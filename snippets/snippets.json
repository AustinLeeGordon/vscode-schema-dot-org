{
    "Breadcrumb List Schema": {
        "prefix": "breadcrumb-list-schema",
        "description": "Breadcrumb List Schema",
        "body": [
            "{",
            "\t\"@context\": \"http://schema.org\",",
            "\t\"@type\": \"BreadcrumbList\",",
            "\t\"itemListElement\": [{",
                "\t\t\"@type\": \"ListItem\",",
                "\t\t\"position\": 1,",
                "\t\t\"name\": \"${1:item-1-name}\",",
                "\t\t\"item\": \"${2:item-1-url}\"",
            "\t},{",
                "\t\t\"@type\": \"ListItem\",",
                "\t\t\"position\": 2,",
                "\t\t\"name\": \"${3:item-2-name}\",",
                "\t\t\"item\": \"${4:item-2-url}\"",
            "\t},{",
                "\t\t\"@type\": \"ListItem\",",
                "\t\t\"position\": 3,",
                "\t\t\"name\": \"${5:item-3-name}\",",
                "\t\t\"item\": \"${6:item-3-url}\"",
            "\t}]",
            "}"
        ]
    },
    "Product Aggregate Offer Schema": {
        "prefix": "product-aggregate-offer-schema",
        "description": "Product Aggregate Offer Schema",
        "body": [
            "{",
            "\t\"@context\": \"http://schema.org\",",
            "\t\"@type\": \"Product\",",
            "\t\"name\": \"${1:product-name}\",",
            "\t\"image\": [",
            "\t\t\"${2:product-images}\"",
            "\t],",
            "\t\"brand\": {",
            "\t\t\"@type\": \"Thing\",",
            "\t\t\"name\": \"${3:product-brand}\"",
            "\t},",
            "\t\"aggregateRating\" : {",
            "\t\t\"@type\": \"AggregateRating\",",
            "\t\t\"ratingValue\": \"${4:rating-value}\",",
            "\t\t\"reviewCount\": \"${5:review-count}\"",
            "\t},",
            "\t\"offers\": {",
            "\t\t\"@type\": \"AggregateOffer\",",
            "\t\t\"lowPrice\": \"${6:low-price}\",",
            "\t\t\"highPrice\": \"${7:high-price}\",",
            "\t\t\"priceCurrency\": \"${8:USD}\",",
            "\t\t\"offerCount\": \"${9:offer-count}\",",
            "\t}",
            "}"
        ]
    },
    "Product Schema": {
        "prefix": "product-schema",
        "description": "Product Schema",
        "body": [
            "{",
            "\t\"@context\": \"http://schema.org\",",
            "\t\"@type\": \"Product\",",
            "\t\"name\": \"${1:product-name}\",",
            "\t\"image\": [",
            "\t\t\"${2:product-images}\"",
            "\t],",
            "\t\"description\": \"${3:product-description}\",",
            "\t\"${4|sku,gtin8,gtin13,gtin14,mpn|}\": \"${5:product-identifier}\",",
            "\t\"brand\": {",
            "\t\t\"@type\": \"Thing\",",
            "\t\t\"name\": \"${6:product-brand}\"",
            "\t},",
            "\t\"aggregateRating\" : {",
            "\t\t\"@type\": \"AggregateRating\",",
            "\t\t\"ratingValue\": \"${7:rating-value}\",",
            "\t\t\"reviewCount\": \"${8:review-count}\"",
            "\t},",
            "\t\"offers\": {",
            "\t\t\"@type\": \"Offer\",",
            "\t\t\"priceCurrency\": \"${9:USD}\",",
            "\t\t\"price\": \"${10:price}\",",
            "\t\t\"priceValidUntil\": \"${11:yyyy-mm-dd}\",",
            "\t\t\"itemCondition\": \"https://schema.org/${12|DamagedCondition,NewCondition,RefurbishedCondition,UsedCondition|}\",",
            "\t\t\"availability\": \"https://schema.org/${13|Discontinued,InStock,InStoreOnly,LimitedAvailability,OnlineOnly,OutOfStock,PreOrder,PreSale,SoldOut|}\",",
            "\t\t\"seller\": {",
            "\t\t\t\"@type\": \"Organization\",",
            "\t\t\t\"name\": \"${14:organization-name}\",",
            "\t\t},",
            "\t\t\"url\": \"${15:offer-url}\"",
            "\t}",
            "}"
        ]
    },
    "Script JSON-LD": {
        "prefix": "script-json-ld",
        "description": "Script JSON-LD",
        "body": [
            "<script type=\"application/ld+json\">",
            "\t${1}",
            "</script>"
        ]
    }
}